    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^common/(.*)$": "<rootDir>/src/common/$1",
      "^entities/(.*)$": "<rootDir>/src/entities/$1",
      "^modules/(.*)$": "<rootDir>/src/modules/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
